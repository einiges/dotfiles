# vim: ft=dosini

# man dunst.5

[global]

# behaviour
follow = keyboard

mouse_left_click   = do_action, remove_current
mouse_middle_click = close_current
mouse_right_click  = close_all

[urgency_low]
timeout     = 5

[urgency_normal]
timeout     = 10
override_pause_level = 30

[urgency_critical]
timeout     = 0
override_pause_level = 60

