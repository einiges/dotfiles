
def_key "g"
  move_home

def_key "G"
  move_end

def_key "d"
  delete_playlist_items

def_key "d"
  delete_stored_playlist

def_key "P"
  stop

def_key "h"
  jump_to_parent_directory

def_key "r"
  toggle_repeat

def_key "R"
  toggle_random

def_key "s"
  toggle_single

def_key "c"
  toggle_consume

def_key "ctrl-v"
  sort_playlist

def_key "ctrl-r"
  reverse_playlist

def_key "n"
  next_found_item

def_key "N"
  previous_found_item

def_key "C"
  clear_main_playlist

def_key "shift-up"
  move_sort_order_up

def_key "shift-up"
  move_selected_items_up

def_key "shift-down"
  move_sort_order_down

def_key "shift-down"
  move_selected_items_down

def_key "z"
  refetch_lyrics

